    "import re # you might find this package useful\n",
    "\n",
    "# Place your code here\n",
    "\n",
    "c=re.findall('\\W(\\d{1,3}) commits*\\W',f)\n",
    "\n",
    "n= re.findall('author=(\\w.[a-z]*[1-9]*[A-Z]*)\\W*',f)\n",
    "\n",
    "contributors ={}\n",
    "\n",
    "x=0\n",
    "for name in n:\n",
    "    contributors[name]=c[x] \n",
    "    x+=1\n",
    "\n",
    "# This line will print your dictionary for grading purposed. Do not remove this line!!!\n",
    "print(contributors)\n",
    "\n",
    "len(contributors)"
